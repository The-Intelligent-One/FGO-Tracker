<?xml version="1.0" encoding="UTF-8"?>

<?import com.github.theintelligentone.fgotracker.ui.valuefactory.BasicServantNameFactory?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.control.*?>
<TabPane xmlns="http://javafx.com/javafx"
         xmlns:fx="http://javafx.com/fxml"
         fx:controller="com.github.theintelligentone.fgotracker.ui.MainWindow"
         prefHeight="400.0" prefWidth="600.0">
    <Tab text="Servants">
        <TableView fx:id="rosterTable">
            <columns>
                <TableColumn text="name">
                    <cellValueFactory>
                        <BasicServantNameFactory/>
                    </cellValueFactory>
                </TableColumn>
                <TableColumn text="level">
                    <cellValueFactory>
                        <PropertyValueFactory property="level"/>
                    </cellValueFactory>
                </TableColumn>
            </columns>
        </TableView>
    </Tab>
</TabPane>